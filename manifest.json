{
  "name": "I Can Do It Better Extension",
  "description": "Extension to make websites work better for me",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://www.visarts.org/classes/*"
      ]
    }
  ]
}